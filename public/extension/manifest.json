{
  "name": "Lunch App Notifications",
  "version": "1.0",
  "manifest_version": 2,
  "background": { "page": "background.html" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "notifications"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}